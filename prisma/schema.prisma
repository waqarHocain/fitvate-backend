generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id           String    @id @default(uuid())
  name         String?
  email        String?   @unique
  password     String?
  profilePic   String?
  mobileNumber String?
  dateofBirth  DateTime?
  gender       String?
  height       String?
  weight       String?
  provider     String?
  googleId     String?   @unique
  facebookId   String?   @unique
}
